{
    "name": "peehaa/async-twitter",
    "homepage": "https://github.com/peehaa/AsyncTwitter",
    "description": "Asynchronous parallel Twitter client built on the Amp concurrency framework",
    "keywords": [
        "twitter",
        "oauth",
        "client",
        "parallel",
        "async",
        "non-blocking"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pieter Hordijk",
            "email": "info@pieterhordijk.com",
            "role": "Lead Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~7.1",
        "amphp/amp": "^2",
        "amphp/artax": "master",
        "daverandom/exceptional-json": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6"
    },
    "autoload": {
        "psr-4": {
            "PeeHaa\\AsyncTwitter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeeHaa\\AsyncTwitterTest\\": "tests/"
        }
    }
}
